{
  "build" : {
    "-D"  : ["no-deprecation-warnings"],
    "-cp" : "src/main/haxe",
    "-lib" : [
      "tink_runloop",
      "stx_pico","stx_nano", "stx_ds", "stx_assert", "stx_fn", "stx_test", "stx_log","stx_log","stx_coroutine","spil"
    ],
    "unit"  : {
      "--macro" : [
        "include('stx.Stream',true)",
        "include('stx.stream',true)"
      ],
      "interp" : {
        "--interp"  : true
      }
    },
    "main" : {
      "-main" : "Main",
      "interp" : {
        "--interp" : true
      }
    },
    "bootstrap" : {
      "-main" : "Bootstrap",
      "interp" : {
        "--interp" : true
      }
    }
  }
}